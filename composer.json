{
    "name": "fsi/admin-security-bundle",
    "type": "symfony-bundle",
    "description": "Authentication & authorization mechanisms for fsi/admin-bundle",
    "keywords": ["admin", "panel", "symfony", "bundle", "fsi", "security"],
    "license": "MIT",
    "authors": [
        {
            "name": "Norbert Orzechowicz",
            "email": "norbert@orzechowicz.pl"
        }
    ],
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/chives/admin-bundle.git"
        }
    ],
    "require": {
        "php": "^7.1|^8.0",
        "fsi/admin-bundle" : "^4.0@dev",
        "symfony/console": "^4.4|^5.0",
        "symfony/dependency-injection": "^4.4|^5.0",
        "symfony/form": "^4.4|^5.0",
        "symfony/framework-bundle": "^4.4|^5.0",
        "symfony/property-access": "^4.4|^5.0",
        "symfony/security": "^4.4|^5.0",
        "symfony/security-bundle": "^4.4|^5.0",
        "symfony/swiftmailer-bundle": "^3.0",
        "symfony/twig-bundle": "^4.4|^5.0",
        "symfony/validator": "^4.4|^5.0",
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.5|^2.0",
        "doctrine/persistence": "^1.3|^2.0"
    },
    "require-dev": {
        "behat/behat": "^3.3",
        "behat/mink": "^1.7",
        "behat/mink-browserkit-driver": "^1.3",
        "behat/mink-extension": "^2.2",
        "behat/mink-selenium2-driver": "^1.3",
        "behat/symfony2-extension": "^2.1",
        "egulias/email-validator": "^2.0",
        "friends-of-phpspec/phpspec-expect": "^3.0|^4.0",
        "phpspec/phpspec": "^5.1|^6.0|^7.0",
        "phpspec/prophecy": "^1.8",
        "phpstan/phpstan": "^0.12.66",
        "sensiolabs/behat-page-object-extension": "^2.0",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/debug": "^4.4|^5.0",
        "symfony/dom-crawler": "^4.4|^5.0",
        "symfony/event-dispatcher": "^4.4|^5.0",
        "symfony/http-kernel": "^4.4|^5.0",
        "symfony/monolog-bridge": "^4.4|^5.0",
        "symfony/monolog-bundle": "^3.0",
        "symfony/routing": "^4.4|^5.0",
        "symfony/twig-bridge": "^4.4|^5.0",
        "symfony/var-dumper": "^4.4|^5.0"
    },
    "config": {
        "bin-dir": "vendor/bin"
    },
    "autoload": {
        "psr-4": {
            "FSi\\Bundle\\AdminSecurityBundle\\": ""
        }
    },
    "autoload-dev": {
        "classmap": ["features/fixtures/project/app/AppKernel.php"],
        "psr-4": {
            "FSi\\Bundle\\AdminSecurityBundle\\Behat\\": "Behat",
            "spec\\FSi\\Bundle\\AdminSecurityBundle\\": "spec/FSi/Bundle/AdminSecurityBundle",
            "FSi\\Bundle\\AdminSecurityBundle\\spec\\fixtures\\": "spec/fixtures"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0-dev",
            "3.2": "3.2-dev",
            "3.1": "3.1-dev",
            "3.0": "3.0-dev",
            "2.0": "2.0-dev",
            "1.0": "1.0-dev"
        }
    }
}
