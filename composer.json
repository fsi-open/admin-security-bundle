{
    "name": "fsi/admin-security-bundle",
    "type": "symfony-bundle",
    "description": "Simple authentication & authorization mechanisms for fsi/admin-bundle",
    "keywords": ["admin", "panel", "symfony", "bundle", "fsi", "security"],
    "license": "MIT",
    "authors": [
        {
            "name": "Norbert Orzechowicz",
            "email": "norbert@orzechowicz.pl"
        }
    ],
    "require": {
        "php": ">=7.1",
        "fsi/admin-bundle" : "^3.0",
        "symfony/console": "^3.0|^4.0",
        "symfony/dependency-injection": "^3.0|^4.0",
        "symfony/form": "^3.0|^4.0",
        "symfony/framework-bundle": "^3.0|^4.0",
        "symfony/property-access": "^3.0|^4.0",
        "symfony/security": "^3.0|^4.0",
        "symfony/security-bundle": "^3.0|^4.0",
        "symfony/swiftmailer-bundle": "^3.0|^4.0",
        "symfony/twig-bundle": "^3.0|^4.0",
        "symfony/validator": "^3.0|^4.0",
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.5",
        "paragonie/random_compat": "^1.1"
    },
    "require-dev": {
        "behat/behat": "^3.3",
        "behat/mink": "^1.7",
        "behat/mink-browserkit-driver": "^1.3",
        "behat/mink-extension": "^2.2",
        "behat/mink-selenium2-driver": "^1.3",
        "behat/symfony2-extension": "^2.1",
        "bossa/phpspec2-expect": "^2.0|^3.0",
        "phpspec/phpspec": "^3.0",
        "phpspec/prophecy": "^1.8",
        "sebastian/comparator": "^3.0",
        "sebastian/exporter": "^3.1",
        "sebastian/recursion-context": "^3.0",
        "sensiolabs/behat-page-object-extension": "^2.0",
        "symfony/class-loader": "^3.0",
        "symfony/dom-crawler": "^3.0|^4.0",
        "symfony/monolog-bridge": "^3.0|^4.0",
        "symfony/monolog-bundle": "^3.0|^4.0",
        "symfony/routing": "^3.0|^4.0",
        "symfony/twig-bridge": "^3.0|^4.0",
        "symfony/var-dumper": "^3.0|^4.0"
    },
    "config": {
        "bin-dir": "vendor/bin"
    },
    "autoload": {
        "psr-4": {
            "FSi\\Bundle\\AdminSecurityBundle\\": ""
        }
    },
    "autoload-dev": {
        "classmap": ["features/fixtures/project/app/AppKernel.php"]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev",
            "2.0": "2.0-dev"
        }
    }
}
